	<ParserLogic>
		<NavigateKey Key="HKEY_LOCAL_MACHINE/SOFTWARE">
			<NavigateKey Key="Autodesk">
				<NavigateKey Key="Architectural Studio">
					<NavigateKey Key="1.0">

						<!-- Autodesk Architectural Studio 1-->

						<ProductDef Name="Autodesk Architectural Studio 1">
							<Discovery>
								<Rule>
									<MatchValue Value="ProductName" Data="Autodesk Architectural Studio"/>
								</Rule>
							</Discovery>
							<Fields>
								<Field Name="ProductName">Autodesk Architectural Studio 1</Field>
								<Field Name="Version"><GetValue Entry="Release" Default="1.0"/></Field>
								<Field Name="Location"><GetValue Entry="Location"/></Field>
								<Field Name="SerialNumber"><GetValue Entry="SerialNumber"/></Field>
								<Field Name="UserNameFirst"><GetValue Entry="FirstName"/></Field>
								<Field Name="UserNameLast"><GetValue Entry="LastName"/></Field>
								<Field Name="Organization"><GetValue Entry="Organization"/></Field>
								<Field Name="DealerName"><GetValue Entry="Dealer"/></Field>
								<Field Name="DealerPhone"><GetValue Entry="DealerPhone"/></Field>
								<Field Name="ProductLanguage"><GetValue Entry="Language"/></Field>
							</Fields>
						</ProductDef>
					</NavigateKey>
					
					<NavigateKey Key="3.0">

						<!-- Autodesk Architectural Studio 3-->

						<ProductDef Name="Autodesk Architectural Studio 3">
							<Discovery>
								<Rule>
									<MatchValue Value="ProductName" Data="Autodesk Architectural Studio"/>
								</Rule>
							</Discovery>
							<Fields>
								<Field Name="ProductName">Autodesk Architectural Studio 3</Field>
								<Field Name="Version"><GetValue Entry="Release" Default="3.0"/></Field>
								<Field Name="Location"><GetValue Entry="Location"/></Field>
								<Field Name="SerialNumber"><GetValue Entry="SerialNumber"/></Field>
								<Field Name="UserNameFirst"><GetValue Entry="CustomerName"/></Field>
								<Field Name="UserNameLast"><GetValue Entry="LastName"/></Field>
								<Field Name="Organization"><GetValue Entry="CustomerCompany"/></Field>
								<Field Name="DealerName"><GetValue Entry="Dealer"/></Field>
								<Field Name="DealerPhone"><GetValue Entry="DealerPhone"/></Field>
								<Field Name="ProductLanguage"><GetValue Entry="Language"/></Field>
							</Fields>
						</ProductDef>
					</NavigateKey>
					
				</NavigateKey>

				<NavigateKey Key="AutoCAD">

					<!-- ALL PRODUCTS UNDER R17.0 KEY -->

					<NavigateKey Key="R17.0">
						<KeyMaskIterate Mask="ACAD-%A:%B">

							<!-- AutoCAD 2007 -->
							<ProductDef Name="AutoCAD 2007">
								<Discovery>
									<Rule>
										<MatchValue Value="ProductName" Data="AutoCAD 2007"/>
									</Rule>
								</Discovery>
								<Fields>
